{
  "logger": {
    "level": "info",
    "timestamp": false
  },
  "esclient": {
    "apiVersion": "5.6",
    "keepAlive": true,
    "requestTimeout": "120000",
    "batchSize": 1,
    "hosts": [{
      "env": "pelias",
      "host": "elasticsearch",
      "port": 9200
    }],
    "log": [{
      "type": "stdio",
      "level": [ "error", "warning" ]
    }]
  },
  "elasticsearch": {
    "settings": {
      "index": {
        "refresh_interval": "10s",
        "number_of_replicas": "0",
        "number_of_shards": "1"
      }
    }
  },
  "schema": {
    "indexName": "pelias"
  },
  "api": {
    "textAnalyzer": "libpostal",
    "services": {
      "placeholder": {
        "url": "http://placeholder:4100"
      },
      "libpostal": {
        "url": "http://libpostal:8080"
      },
      "pip": {
        "url": "http://pip-service:4200"
      },
      "interpolation": {
        "url": "http://interpolation:4300"
      }
    }
  },
  "imports": {
    "adminLookup": {
      "enabled": true
    },
    "geonames": {
      "datapath": "/data/geonames"
    },
    "gndb": {
      "datapath": "/data/gndb"
    },
    "openstreetmap": {
      "leveldbpath": "/tmp",
      "datapath": "/data/openstreetmap"
    },
    "openaddresses": {
      "datapath": "/data/openaddresses"
    },
    "polyline": {
      "datapath": "/data/polylines"
    },
    "whosonfirst": {
      "datapath": "/data/whosonfirst",
      "importVenues": false,
      "importPostalcodes": false,
      "importPlace": "101715829",
      "polygons": false,
      "simplifyPolygons": false
    },
    "interpolation": {
      "download": {
        "tiger": {
          "datapath": "/data/tiger"
        }
      }
    }
  }
}
